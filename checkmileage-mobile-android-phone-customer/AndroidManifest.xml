		<activity android:name="Settings_TermsPageActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light" ></activity>
		<activity android:name="Settings_LeavePageActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light" ></activity>
		<activity android:name="SettingsPageActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light" ></activity>
