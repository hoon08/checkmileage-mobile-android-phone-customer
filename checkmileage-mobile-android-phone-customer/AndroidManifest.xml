	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<application
	     android:icon="@drawable/icon"
	     android:label="@string/AppName"
	    >
	 	<uses-library android:name="com.google.android.maps" />
	    <activity android:name="CreateQRPageActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light" ></activity>
	    <activity android:name="Main_TabsActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light"  
	         android:windowSoftInputMode="adjustPan"
	        ></activity>
	    <activity android:name="MainActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light"  >
	    </activity>
	    <activity android:name="MemberStoreInfoPage" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light" ></activity>
	    <activity android:name="MemberStoreListPageActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light" 
	        
	        ></activity>
